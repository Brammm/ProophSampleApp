{
    "autoload": {
        "psr-4": {
            "Todo\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Todo\\Tests\\": "tests/"
        }
    },
    "require": {
        "php": ">=7.2",
        "doctrine/dbal": "^2.7",
        "mnapoli/silly-php-di": "^1.2",
        "php-di/php-di": "^6.0",
        "php-di/slim-bridge": "^2.0",
        "prooph/event-sourcing": "^5.5",
        "prooph/event-store-bus-bridge": "^3.2",
        "prooph/pdo-event-store": "^1.8",
        "ramsey/uuid": "^3.7",
        "vlucas/phpdotenv": "^2.4"
    },
    "require-dev": {
        "phpunit/phpunit": "^7.2",
        "roave/security-advisories": "dev-master"
    },
    "config": {
        "sort-packages": true
    },
    "scripts": {
        "streams:create": "docker exec -i prooph-tutorial_todo-php-fpm_1 bin/todo streams:create"
    }
}
